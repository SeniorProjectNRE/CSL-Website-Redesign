@{
    ViewBag.Title = "CSL - Jobs at the California State Library";
    ViewBag.PageTitle = "Jobs at the California State Library";
    ViewBag.Author = "Ryan Salcido, Matt Loller";
    ViewBag.Description = "When you become eligible to work for the state (when you have passed an Open Examination), or if you are a former or current state employee, you can apply for a job at the California State Library. The California Department of Human Resources  lists these jobs for us on their website CalCareers https://jobs.ca.gov/Default.aspx.";
    ViewBag.Keywords = "vacant position, vacancy, examination, current state employee, former state employee, new to state";
}
<head>
    <title>@(ViewBag.Title)</title>
    <meta name="Author" content="@(ViewBag.Author)" />
    <meta name="Description" content="@(ViewBag.Description)" />
    <meta name="Keywords" content="@(ViewBag.Keywords)" />
    <link rel="stylesheet" href="~/Content/StateTemplate/css/custom.css" media="screen, projection, print, tv" type="text/css" />
    <link rel="stylesheet" href="~/Content/StateTemplate/css/custom1.css" media="screen, projection, print, tv" type="text/css" />
    <link rel="stylesheet" href="~/Content/StateTemplate/css/random.css" media="screen, projection, print, tv" type="text/css" />
</head>

<style>
    @@media (min-width: 1280px) {
        #title-page {
            display: none;
        }

        #page-banner {
            display: block;
        }
    }

    @@media (max-width: 767px) {
        #title-page {
            display: block;
        }

        #page-banner {
            display: none;
        }
    }


    .holidays {
        width: 30%;
        float: right;
        margin-top: 30px;
        text-align: center;
        border: 1px solid #c7c7c7;
        padding: 5px 30px
    }

    .blue-div {
        background-color: #0a5b82;
        width: 100%;
        text-align: center;
        padding-bottom: 30px;
        margin: 0 auto;
        font-weight:normal;
        color:black;
    }

        .blue-div h2 {
            color: #fff;
            text-transform: uppercase;
            font-weight: bold;
            padding: 20px 0 0 0;
            text-align: left;
        }

        .blue-div p {
            color: #046b99;
        }

    .threecolumns {
        /*width: 1246px;*/
        margin: 0 auto;
        height: auto;
    }

    .columns {
        width: 400px;
        float: left;
        margin-right: 22px;
        background-color: #fff;
    }

        .columns p, .twocolumns p {
            padding: 20px;
        }

        .columns img {
            width: 400px
        }

        .columns h3 {
            text-align: left;
            font-size: 1.2em;
            margin: 10px 0;
        }

    .twocolumns {
        width: 600px;
        float: left;
        margin-right: 22px;
        background-color: #fff;
    }

    .wrap {
        padding: 15px !important
    }

    .icon {
        width: 33px;
        float: left;
        padding: 20px 0
    }

    .more-info {
        padding-top: 20px
    }

    .grey-box {
        width: 100%;
        background-color: #e1e1e1;
        height: 55px;
        text-align: center;
    }

    .box-with-border {
        width: 23%;
        float: right;
        margin: 30px 60px 0 0;
        text-align: center;
        border: 1px solid #c7c7c7;
        padding: 5px 30px
    }

    .orange {
        color: #FDB81E;
        font-weight: bold
    }

    .blue {
        text-align: center;
        color: #0a5b82;
        font-weight: bold
    }

    .expand-div {
        width: 33%;
        float: left;
        padding-right: 20px;
        margin: 0 auto;
    }

    .panel-group {
        margin-bottom: 0
    }

    .spacer {
        height: 30px;
    }

    .center {
        text-align: center;
        vertical-align: bottom;
        bottom: 0;
        position: relative;
    }

    .header {
        text-align: left;
    }

    blockquote {
        font-size:1.15em;
    }
</style>

<div id="main-content" class="main-content">

    <div class="container">
        <main class="main-primary">
            <!-- InstanceBeginEditable name="MainContent" -->
            <ol class="breadcrumb">
                <li><a href="/">Home</a></li>
                <li><a href="/work-with-us/">Work With Us</a></li>
                <li class="active">Jobs</li>
            </ol>
            <h1 id="title-page">@(ViewBag.PageTitle)</h1>
            <img id="page-banner" src="~/Content/StateTemplate/images/topcoming.jpg" style="width:100%" alt="Work With Us" title="Work With Us" />
            <div class="half" style="padding:20px 0">
                <p>
                    When you become <a href="http://jobs.ca.gov/GeneralInfo/Glossary#class-E" target="_blank" rel="noopener noreferrer">eligible</a> to work for the state (when you have passed an <a href="http://jobs.ca.gov/" target="_blank" rel="noopener noreferrer">Open Examination</a>), or if you are a former or current state employee, you can apply for a job at the California State Library. The <a href="https://www.jobs.ca.gov/" target="_blank" rel="noopener noreferrer">California Department of Human Resources (CalHR)</a> lists these jobs for us on their website.
                </p>
            </div>
            <div class="half">
                <div class="animate-fadeInRight">
                    <div class="holidays">
                        <a href="https://jobs.ca.gov/CalHRPublic/Search/JobSearchResults.aspx#depid=276" target="_blank" rel="noopener noreferrer">CalCareers Postings</a>
                    </div>
                </div>
            </div>
            <div style="clear: both;"></div>
        </main>
    </div>
    <div class="blue-div">
        <div class="threecolumns">
            <h2>Current Job Openings</h2>
            <div class="half">
                <section class="exams-list">
                    <article class="exam-item">
                        <div class="header">
                            <div class="title"><a href="https://jobs.ca.gov/CalHrPublic/Jobs/JobPosting.aspx?JobControlId=79395" target="_blank" rel="noopener noreferrer">Library Programs Consultant (2 positions available) &ndash; <br /> Library Development Services</a></div>
                            <div class="filing-date">Final Filing Date <time datetime="2017-10-17">10/20/2017</time></div>
                        </div>
                        <div class="body">
                            <!--<div class="id">ID: <span>2161</span></div>
                            <div class="base">Base: <span>open</span></div>-->
                        </div>
                        <div class="footer">
                            <div class="published">Published: <time datetime="2017-08-23">September 22, 2017</time></div>
                            <a href="http://www.library.ca.gov/about/jobs/docs/3642.JD.LPC.Rev9.21.17.pdf" target="_blank" rel="noopener noreferrer" class="btn btn-default">LPC Duty Statement</a>
                        </div>
                    </article>
                </section>
            </div>
            <div class="half">
                <section class="exams-list">
                    <article class="exam-item">
                        <div class="header">
                            <div class="title"><a href="http://www.library.ca.gov/about/jobs/docs/JOB-Fiscal-SSMII_FFDExtended.pdf" target="_blank" rel="noopener noreferrer">Staff Services Manager II (Supervisory) &ndash; <br /> Administrative Services Bureau / Fiscal Services Section</a></div>
                            <div class="filing-date">Final Filing Date <time datetime="2017-09-21">09/21/2017</time></div>
                        </div>
                        <div class="body">
                            <!--<div class="id">ID: <span>2179</span></div>
                            <div class="base">Base: <span>open</span></div>-->
                        </div>
                        <div class="footer">
                            <div class="published">Published: <time datetime="2017-07-28">July 28, 2017</time></div>
                            <a href="http://www.library.ca.gov/about/jobs/docs/3642.JD.LPC.Rev9.21.17.pdf" target="_blank" rel="noopener noreferrer" class="btn btn-default">LPC Duty Statement</a>
                        </div>
                    </article>
                </section>
            </div>
            <div style="clear:both"></div>
            <ul class="list-overstated accordion-list" style="text-align:left;  width:98%">
                <li>
                    <a class="toggle" style="color:#fff;">Application Instructions</a>
                    <div class="description" style="background-color:#e2f5ff">
                        <blockquote>
                            <div class="half">
                                <p><strong>You may send job applications to</strong></p>
                                <p>
                                    The California State Library<br />
                                    Human Resources Services<br /><br />
                                    P.O. Box 942837,
                                    Sacramento, CA 94237-0001
                                </p>
                            </div>
                            <div class="half">
                                <p><strong>You may hand-deliver job applications to</strong></p>
                                <p>
                                    The California State Library,
                                    Human Resources Services<br />
                                    California State Library Building<br /><br />
                                    900 N Street, Suite 400,
                                    Sacramento, CA 95814
                                </p>
                            </div>
                            <div style="clear:both"></div>
                            <div class="spacer"></div>
                            <p>All applicants must clearly indicate the basis for their eligibility in "Examination(s) or Job Title(s) for which you are applying" on the standard application form (STD678).</p>
                            <p>All appointments subject to State Restrictions of Appointments (SROA) provisions. Surplus employees are encouraged to apply.</p>
                            <p>Applications will be screened and only the most qualified candidates will be interviewed.</p>
                            <p>For further information on employment at the State Library, email <a href="mailto:csljobs@library.ca.gov">csljobs@library.ca.gov</a> or call <a href="tel:+19166540202">(916) 654-0202</a>.</p>
                        </blockquote>
                    </div>
                </li>
            </ul>
            <div style="clear:both"></div>
        </div>
    </div>
    <div class="spacer"></div>
    <div class="threecolumns">
        <div class="half">
            <div class="card card-default">
                <img class="card-img-top img-responsive" src="~/Content/StateTemplate/images/comingsoon.jpg" alt="Intern">
                <div class="card-block">
                    <h4 class="card-title">INTERNSHIPS</h4>
                    <p>Learn about internship opportunities designed to give undergraduate students meaningful work experience as well as potentially college credit.</p>
                    <a href="/work-with-us/internships/" class="btn btn-default">MORE ABOUT INTERNSHIPS</a>
                </div>
            </div>
        </div>
        <div class="half">
            <div class="card card-default">
                <img class="card-img-top img-responsive" src="~/Content/StateTemplate/images/comingsoon.jpg" alt="Volunteer">
                <div class="card-block">
                    <h4 class="card-title">VOLUNTEER</h4>
                    <p> Learn about current volunteer opportunities at one of oldest cultural institutions in California.</p>
                    <a href="/work-with-us/volunteer/" class="btn btn-default">MORE ABOUT VOLUNTEERING</a>
                </div>
            </div>
        </div>
        <div style="clear:both"></div>

        <h2 class="orange">Currently scheduled examinations</h2>
