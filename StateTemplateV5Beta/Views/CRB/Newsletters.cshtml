
@{
    ViewBag.Title = "CSL - California Research Bureau Newsletters";
    ViewBag.Author = "California State Library";
    ViewBag.Description = "Subscribe to the California Research Bureau's free, weekly newsletters, Lunch with a side of Research and Studies in the News.";
    ViewBag.Keywords = "California, state, Library, Policies";
}

<head>
    <title>@(ViewBag.Title)</title>
    <meta name="Author" content="@(ViewBag.Author)" />
    <meta name="Description" content="@(ViewBag.Description)" />
    <meta name="Keywords" content="@(ViewBag.Keywords)" />

    <link rel="stylesheet" href="~/Content/StateTemplate/css/custom.css" media="screen, projection, print, tv" type="text/css" />
    <link rel="stylesheet" href="~/Content/StateTemplate/css/custom1.css" media="screen, projection, print, tv" type="text/css" />
    <link rel="stylesheet" href="~/Content/StateTemplate/css/random.css" media="screen, projection, print, tv" type="text/css" />
</head>

<style>
    @@media (min-width: 1280px) {
        #title-page {
            display: none;
        }

        #page-banner {
            display: block;
        }
    }

    @@media (max-width: 767px) {
        #title-page {
            display: block;
        }

        #page-banner {
            display: none;
        }
    }

    .link-icon img {
        width:auto;
    }

    .color-box {
        height:auto;
        padding-bottom:0;
    }

    .transp-p {
        text-align: center;
        color: #fff;
        opacity: 0.5;
        padding: 0;
        margin-top: 10px;
        font-size:1.1em;
    }

    .white-title {
        margin:0;
    }

    .blue-div {
        font-weight: normal;
        color: #333333;
    }
        .blue-div p {
            color: #333333;
        }
    .panel-default > .panel-heading {
        background-color: #0a5b82;
        border: 0;
    }

    .panel-title > a {
        color: #fff;
        font-size: 2.2em;
    }

    .panel-default .panel-body {
        background-color: #d8e2eb;
    }

    .accordion .panel-heading .panel-title a:after, .panel-group .panel-heading .panel-title a:after, .service-tile-content .panel-heading .panel-title a:after {
        color: #fff;
        top: 36%;
    }
</style>

