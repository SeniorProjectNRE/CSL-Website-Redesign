
@{
    ViewBag.Title = "California State Library – Disposal List Instructions";
    ViewBag.Author = "Dmitriy Rusetskiy";
    ViewBag.Description = "Disposal list instructions for Federal Depository Program Libraries include federal legal requirements for discarding and Regional rules on formatting.";
    ViewBag.Keywords = "California, state, Library, Policies";
}

<head>
    <title>@(ViewBag.Title)</title>
    <meta name="Author" content="@(ViewBag.Author)" />
    <meta name="Description" content="@(ViewBag.Description)" />
    <meta name="Keywords" content="@(ViewBag.Keywords)" />

    <link rel="stylesheet" href="~/Content/StateTemplate/css/custom.css" media="screen, projection, print, tv" type="text/css" />
    <link rel="stylesheet" href="~/Content/StateTemplate/css/custom1.css" media="screen, projection, print, tv" type="text/css" />
    <link rel="stylesheet" href="~/Content/StateTemplate/css/random.css" media="screen, projection, print, tv" type="text/css" />
</head>

<div id="main-content" class="main-content">
    <main class="main-primary">
        <div class="container">
            <ol class="breadcrumb dynamic"></ol>
            <h2 class="page-title">Disposal List Instructions for California Selective FDLP Libraries</h2>
        </div>

        <div class="section">
            <div class="container">
                <p>Please send your FDLP disposal lists as e-mail attachments to the Regional FDLP Librarian at <a href="mailto:FDLP.Regional@library.ca.gov">FDLP.Regional@library.ca.gov</a>. Please attach only one disposal list to each e-mail message. You may submit more than one disposal list at a time, but use a separate e-mail message for each list.</p>
            </div>
        </div>
    </main>
</div>


