@model CSLBusinessObjects.ViewModels.GrantsViewModel

@{
    ViewBag.Title = "PreviousGrantAwards";
}

<head>
    <link href='~/Content/StateTemplate/css/select2.min.css' rel='stylesheet' type='text/css'>
    <script src='/Content/StateTemplate/js/libs/select2.min.js' type="text/javascript"></script>
    <style>
        select, input{
            color:black;
        }
    </style>
</head>

<div id="main-content" class="main-content">
    <main class="main-primary">
        <div class="section">
            <div class="container">
                <h2>PreviousGrantAwards</h2>
                <button type="submit" id="submit">Generate</button>

                <table id="grantsData">
                    <thead>
                        <tr>
                            <th>Grant#</th>
                            <th>Year</th>
                            <th>Library</th>
                            <th>Project</th>
                            <th>Award</th>
                            <th>Narrative</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.GrantGetAllList)
                        {
                            <tr>
                                <td>@item.GrantID</td>
                                <td>@item.Year</td>
                                <td>@item.Library</td>
                                <td>@item.Project</td>
                                <td>@item.Award</td>
                                <td>row</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
                
            
    </main>
</div>

<script>
    $(document).ready(function () {
        $('#grantsData').DataTable();
    });
</script>

